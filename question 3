import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
		System.out.println("enter size of  array");
		int a=sc.nextInt();
		int arr[]=new int[a];
		System.out.println("enter array");
		for(int i=0;i<a;i++){
		    arr[i]=sc.nextInt();
		}
		for(int i=a-1;i>=0;i--){
		    System.out.print(" "+arr[i]);
		}
	}
}
